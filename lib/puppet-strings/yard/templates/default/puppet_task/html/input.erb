<% if json['input_method'] || json['supports_noop'] %>
<div class="tags">
  <p class="tag_title"> Input </p>
  <ul>
    <% if json['input_method'] %>
        <li> Input method: <%= json['input_method'] %> </li>
    <% end %>
        <li> Supports noop? <%= json['supports_noop'] ? "true" : "false" %></li>
  </ul>
</div>
<% end %>
